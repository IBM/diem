        "del": "^7.0.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-angular": "^4.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^46.4.4",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-rxjs": "^5.0.3",
        "eslint-plugin-sonarjs": "^0.19.0",
        "prettier": "^3.0.0",
        "rxjs": "^7.8.1",
