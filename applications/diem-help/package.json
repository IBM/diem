    "scripts": {
        "build": "./node_modules/.bin/npm-run-all clean clean:front lock gulp:aot-ts ivy webpack:front webpack:server webpack:node",
        "build:local": "npm-run-all clean clean:front -p gulp:default webpack:front-local webpack:server webpack:node-local",
        "clean": "rimraf server",
        "clean:front": "rimraf public/js ",
        "compodoc": "./node_modules/.bin/compodoc -p src/client/tsconfig.json",
        "docker:build": "docker build -t $npm_package_config_containername -f deploy/Dockerfile .",
        "docker:build-local": "docker build -t $npm_package_config_containername -f deploy/Dockerfile.local .",
        "docker:build-production": "docker build -t $npm_package_config_containernameprod -f deploy/Dockerfile .",
        "docker:push": "docker push $npm_package_config_containername",
        "docker:push-production": "docker push $npm_package_config_containernameprod",
        "gulp": "tsc --module commonjs --target ES6 --lib es6,dom gulpfile.ts",
        "gulp:default": "gulp default",
        "gulp:aot-ts": "gulp aot-ts",
        "kube:dot": "node ../../common/k8s/dot",
        "kube:init": "node ../../common/k8s/init",
        "kube:init-prod": "node ../../common/k8s/init-prod",
        "kube:local": "npm-run-all clean clean:front docker:build-local docker:push",
        "kube:local-prod": "./node_modules/.bin/npm-run-all build docker:build docker:push",
        "kube:production": "npm-run-all build docker:build-production docker:push-production",
        "lock": "npm install --package-lock",
        "start": "node ./server/server.js",
        "test": "jest --verbose --no-cache",
        "webpack:front": "cross-env webpackenv=front webpack --mode  production",
        "webpack:front-local": "cross-env webpackenv=front-local node --trace-deprecation ./node_modules/.bin/webpack -w --mode development",
        "webpack:node": "webpackenv=node webpack  --mode production",
        "webpack:node-local": "cross-env webpackenv=node-local webpack -w   --progress --mode development",
        "webpack:node-test": "cross-env webpackenv=node-test webpack -w   --progress --mode development",
        "webpack:server": "cross-env webpackenv=server webpack  --mode production",
        "eslint:node": "eslint -c ./src/server/.eslintrc.js ./src/server/ --ext .ts",
        "eslint:front": "eslint -c ./src/client/.eslintrc.js ./src/client/ --ext .ts --ignore-pattern *.pug.tmpl.ts",
        "eslint": "npm-run-all eslint:node eslint:front",
        "ivy": "./node_modules/.bin/ngcc"
    }
