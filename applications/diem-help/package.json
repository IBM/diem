    "private": true,
    "scripts": {
        "build": "./node_modules/.bin/npm-run-all clean clean:front lock pug:aot-ts webpack:build webpack:front webpack:node",
        "clean": "rimraf server",
        "clean:front": "rimraf public/js public/assets public/fonts public/skins public/css ",
        "compodoc": "./node_modules/.bin/compodoc -p src/client/tsconfig.json",
        "docker:build": "docker build -t $npm_package_config_containername -f docker/Dockerfile .",
        "docker:build-local": "docker build -t $npm_package_config_containername -f docker/Dockerfile.local .",
        "docker:build-production": "docker build -t $npm_package_config_containernameprod -f docker/Dockerfile .",
        "docker:push": "docker push $npm_package_config_containername",
        "docker:push-production": "docker push $npm_package_config_containernameprod",
        "pug:aot-ts": "aot-fixer-pug --src-path src/client",
        "pug:watch": "nodemon --watch './src/**/*' -e pug --exec npm run pug:aot-ts",
        "kube:dot": "node ../../common/k8s/dot",
        "kube:init": "node ../../common/k8s/init",
        "kube:init-prod": "node ../../common/k8s/init-prod",
        "kube:local": "./node_modules/.bin/npm-run-all build docker:build docker:push",
        "kube:local-n": "./node_modules/.bin/npm-run-all webpack:node docker:build docker:push",
        "kube:local-d": "./node_modules/.bin/npm-run-all docker:build docker:push",
        "lock": "npm install --package-lock",
        "start": "node ./server/server.js",
        "test": "jest --verbose --no-cache",
        "webpack:build": "rm -rf ./src/webpack/lib && ./node_modules/.bin/tsc -p ./src/webpack/tsconfig.json",
        "webpack:front": "cross-env webpackenv=front webpack --mode  production",
        "webpack:front-local": "cross-env webpackenv=front-local node --trace-deprecation ./node_modules/.bin/webpack -w --mode development",
        "webpack:node": "webpackenv=node webpack  --mode production",
        "webpack:node-local": "cross-env webpackenv=node-local webpack -w   --progress --mode development",
        "webpack:node-test": "cross-env webpackenv=node-test webpack -w   --progress --mode development",
        "eslint:node": "eslint -c ./src/server/.eslintrc.js ./src/server/ --ext .ts",
        "eslint:front": "eslint -c ./src/client/.eslintrc.js ./src/client/ --ext .ts --ignore-pattern *.pug.tmpl.ts",
        "eslint": "npm-run-all eslint:node eslint:front",
        "eslint:node-fix": "eslint -c ./src/server/.eslintrc.js ./src/server/ --fix --ext .ts",
        "eslint:front-fix": "eslint -c ./src/client/.eslintrc.js ./src/client/ --fix --ext .ts --ignore-pattern *.pug.tmpl.ts",
        "eslint-fix": "npm-run-all eslint:node-fix eslint:front-fix",
        "depcheck": "npx depcheck .  --specials=eslint,tslint,ttypescript,webpack,prettier"
    }
}
